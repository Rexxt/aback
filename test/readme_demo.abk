main: /* define function main */
    " Hello, world! " print /* push Hello, world! to the stack, print it and drop it. */
;; /* end def */
main

0 $n
n dup /* push the value of n to the stack twice */
print /* print the first occurence */
1 + $n /* , and increase the other by one and push it back into n */
n print /* and finally show it to the screen */